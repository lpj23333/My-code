#include <stdio.h>
 int main()
 
 {
 	int hour1,minute1;
 	int hour2,minute2;
 	
 	printf("请输入开始时间");
	 scanf("%d %d",&hour1,&minute1);
	 
	printf("请输入结束时间") ;
	 scanf("%d %d",&hour2,&minute2);
	 
	int ih=hour2-hour1;
	int im=minute2-minute1;
	
    if (im<0){
    	im=im+60;
    	ih --;
	}
	
	printf("时间差为%d小时%d分",ih,im);
 	return 0;
 }
